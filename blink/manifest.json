{
  "name": "BabyAISecurity",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "serviceWorker.js"
  },
  "content_scripts": [
    {
      "js": [
        "content/content.js"
      ],
      "css": [
        "content/content.css"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "permissions": [
    "storage",
    "scripting",
    "tabs",
    "activeTab"
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "assets/fonts/*.*",
        "wrongpage/index.html",
        "wrongpage/style.css",
        "assets/images/Lock.png",
        "assets/utils/apiRoutes.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup/index.html",
    "default_title": "Открыть меню"
  },
  "icons": {
    "16": "assets/images/icon16.png",
    "32": "assets/images/icon32.png",
    "48": "assets/images/icon48.png",
    "128": "assets/images/icon128.png"
  }
}